#!/usr/bin/python3

import sys

# Function to factorize a number into the product of two smaller numbers
def factorize(n):
  # Check if n is even
  if n % 2 == 0:
    return 2, n // 2
  # If n is odd, try dividing it by odd numbers starting from 3
  for i in range(3, n, 2):
    if n % i == 0:
      return i, n // i
  # If no smaller factors were found, return n and 1
  return n, 1

# Read the file name from the command line
file = sys.argv[1]

# Open the file and read the numbers
with open(file, 'r') as f:
  for line in f:
    # Convert the line to an integer and factorize it
    n = int(line)
    p, q = factorize(n)
    # Print the factorization
    print(f"{n} = {p} * {q}")
